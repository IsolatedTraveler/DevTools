{
  "presets":[
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "targets": {
          "browsers": ["> 1%", "last 20 versions", "not ie <= 8", "safari 7"]
        }
      }
    ]
  ],
  "plugins":[
    "@babel/plugin-transform-object-assign",
    "@babel/plugin-transform-runtime"
  ]
}